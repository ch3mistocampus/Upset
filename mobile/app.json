{
  "expo": {
    "name": "Upset",
    "slug": "upset",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "automatic",
    "scheme": "upset",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#F6F7F9"
    },
    "jsEngine": "jsc",
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.getupset.app",
      "usesAppleSignIn": true,
      "infoPlist": {
        "NSCameraUsageDescription": "Used for profile photos",
        "NSPhotoLibraryUsageDescription": "Used for profile photos"
      }
    },
    "android": {
      "package": "com.getupset.app",
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#F6F7F9"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "permissions": []
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-apple-authentication",
      "@sentry/react-native",
      [
        "@sentry/react-native/expo",
        {
          "url": "https://sentry.io/",
          "project": "react-native",
          "organization": "upset"
        }
      ],
      "expo-font"
    ],
    "extra": {
      "eas": {
        "projectId": "0e4bc5ef-48da-4080-817c-b069d6d5d7ec"
      },
      "router": {}
    },
    "owner": "chemistoncampusx",
    "updates": {
      "enabled": true,
      "fallbackToCacheTimeout": 0,
      "url": ""
    },
    "runtimeVersion": {
      "policy": "appVersion"
    }
  }
}
