{
  "expo": {
    "name": "UFC Picks Tracker",
    "slug": "ufc-picks-tracker",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "automatic",
    "scheme": "ufcpicks",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#F6F7F9"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.ufcpicks.tracker",
      "infoPlist": {
        "NSCameraUsageDescription": "Used for profile photos",
        "NSPhotoLibraryUsageDescription": "Used for profile photos"
      }
    },
    "android": {
      "package": "com.ufcpicks.tracker",
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#F6F7F9"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "permissions": []
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "@sentry/react-native/expo",
      "expo-router"
    ],
    "extra": {
      "eas": {
        "projectId": ""
      }
    },
    "owner": "",
    "updates": {
      "enabled": true,
      "fallbackToCacheTimeout": 0,
      "url": ""
    },
    "runtimeVersion": {
      "policy": "appVersion"
    }
  }
}
